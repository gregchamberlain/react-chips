{
  "name": "react-component-boilerplate",
  "version": "0.1.0",
  "description": "A simple responsive sidebar component for React",
  "keywords": [
    "react",
    "react-component"
  ],
  "main": "dist/bundle.js",
  "scripts": {
    "start": "webpack-dev-server --open",
    "lint": "eslint src",
    "test": "jest --coverage",
    "test:watch": "npm run test -- --watch",
    "build": "NODE_ENV=production ./build-tools/build.sh",
    "compile": "NODE_ENV=production webpack --config build-tools/webpack.config.prod.js --progress",
    "buildSite": "NODE_ENV=production webpack --config build-tools/webpack.config.site.js --progress",
    "prepublish": "npm run build && npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gregchamberlain/react-component-boilerplate.git"
  },
  "author": "Greg Chamberlain <gregchamberlain94@gmail.com> (https://gregchamberlain.github.io)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gregchamberlain/react-component-boilerplate/issues"
  },
  "homepage": "https://github.com/gregchamberlain/react-component-boilerplate#readme",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.5.2",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.16.0",
    "eslint-plugin-react": "^6.10.0",
    "jest": "^19.0.1",
    "react": "^15.3.0",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.3.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.4.2",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  },
  "peerDependencies": {
    "react": "^15.3.0",
    "react-dom": "^15.3.0"
  },
  "dependencies": {}
}
